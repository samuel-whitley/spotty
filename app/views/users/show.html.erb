<p class="notice"><%= notice %></p>

<h1> Profile Menu </h1>
<br>

<p> Username: <%= @user.email %> </p>

<% if @user.join_votes %>
<ul>
  <% @user.join_votes.each do |g| %>
    <li><%= link_to "Song #{song.id}", song_path(g) %></li>
  <% end %>
</ul>
<% else %>
  <div>"You haven't voted for any songs!"</div>
<% end %>

